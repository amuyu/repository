<?xml version="1.0" encoding="UTF-8"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.bitcoinj</groupId>
  <artifactId>bitcoinj-core</artifactId>
  <version>0.16.1</version>
  <name>bitcoinj</name>
  <description>A Java Bitcoin library</description>
  <url>https://bitcoinj.org/</url>
  <licenses>
    <license>
      <name>The Apache Software License, Version 2.0</name>
      <url>https://www.apache.org/licenses/LICENSE-2.0.txt</url>
    </license>
  </licenses>
  <developers>
    <developer>
      <name>The bitcoinj developers</name>
      <email>bitcoinj@googlegroups.com</email>
    </developer>
  </developers>
  <scm>
    <connection>scm:git:https://github.com/bitcoinj/bitcoinj</connection>
    <developerConnection>scm:git:git@github.com:bitcoinj/bitcoinj.git</developerConnection>
    <url>https://github.com/bitcoinj/bitcoinj</url>
